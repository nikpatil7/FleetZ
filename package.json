{
  "name": "smart-delivery",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "server": "npm --workspace backend run dev",
    "client": "npm --workspace frontend run dev"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
